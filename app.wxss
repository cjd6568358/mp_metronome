.container-dark .weui-half-screen-dialog {
  background: #191919
}

.container-dark .weui-half-screen-dialog .weui-half-screen-dialog__hd {
  border-bottom: .5px solid #333d52
}

.container-dark .weui-dialog__title {
  color: #fff
}

.container-dark .weui-cell_swiped>.weui-cell__bd {
  background-color: #191919
}

@-webkit-keyframes caret {
  50% {
    opacity: 0
  }
}

@keyframes caret {
  50% {
    opacity: 0
  }
}

@-webkit-keyframes fadeinout {
  0% {
    opacity: 0
  }

  100% {
    opacity: 1
  }
}

@keyframes fadeinout {
  0% {
    opacity: 0
  }

  100% {
    opacity: 1
  }
}

@media screen and (min-aspect-ratio:375/667) {
  .weui-half-screen-dialog .weui-half-screen-dialog__hd {
    height: 44rpx;
    min-height: 44px
  }

  .weui-half-screen-dialog__title {
    font-size: 28rpx
  }
}

@media screen and (min-aspect-ratio:375/667) and (min-width:768px) {
  .backnav .back-icon {
    padding: 10rpx
  }

  .weui-half-screen-dialog .weui-half-screen-dialog__hd {
    height: 60rpx;
    min-height: 60px
  }
}

@media screen and (max-aspect-ratio:6/13) {
  .container {
    min-height: 95vh
  }
}

@media screen and (max-aspect-ratio:441/970) {
  .container {
    min-height: 95vh
  }
}