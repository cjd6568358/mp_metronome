}

.container-dark.theme-red {
  background: #1c1c1e linear-gradient(166deg, #461b1a, #1c1c1e 50%, #1c1c1e)
}

.container-dark.theme-light-green {
  background: #1c1c1e linear-gradient(166deg, #478842, #1c1c1e 50%, #1c1c1e)
}

.container-dark.theme-purple {
  background: #1c1c1e linear-gradient(166deg, #7047cc, #1c1c1e 50%, #1c1c1e)
}

.container-dark.theme-pink {
  background: #1c1c1e linear-gradient(166deg, #a3459f, #1c1c1e 50%, #1c1c1e)
}

.container-dark.theme-dark-blue {
  background: #1c1c1e linear-gradient(166deg, #2f6c84, #1c1c1e 50%, #1c1c1e)
}

.container-dark.theme-green {
  background: #1c1c1e linear-gradient(166deg, #326136, #1c1c1e 50%, #1c1c1e)
}

.container-dark.theme-light-red {
  background: #1c1c1e linear-gradient(166deg, #a65264, #1c1c1e 50%, #1c1c1e)
